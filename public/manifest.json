{
  "name": "Bookie - Restaurant & Hotel Booking",
  "short_name": "Bookie",
  "description": "Book your favorite restaurants and hotels with ease",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#0891b2",
  "orientation": "portrait-primary",
  "icons": [
    {
      "src": "/icons/icon-96x96.svg",
      "sizes": "96x96",
      "type": "image/svg+xml",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-192x192.svg",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-512x512.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "maskable any"
    }
  ],
  "categories": ["food", "travel", "lifestyle"],
  "lang": "en",
  "dir": "ltr",
  "scope": "/",
  "id": "bookie-app",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "display_override": ["window-controls-overlay"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "screenshots": [
    {
      "src": "/icons/icon-512x512.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "form_factor": "wide",
      "label": "Bookie App"
    }
  ]
}